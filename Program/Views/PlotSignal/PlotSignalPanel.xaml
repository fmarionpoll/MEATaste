<UserControl x:Class="MEATaste.Views.PlotSignal.PlotSignalPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             HorizontalAlignment="Stretch"
             VerticalAlignment="Stretch"
             Background="Transparent">
             

    <Grid Margin="0,0,0,0"  Background="#FFFFFF" Name="ScottPlotGrid" >
        <!--<WpfPlot x:Name = "PlotControl" AxesChanged = "PlotControl_AxesChanged" />-->
        <ContentControl Content="{Binding PlotControl}" />
    </Grid>

</UserControl>
