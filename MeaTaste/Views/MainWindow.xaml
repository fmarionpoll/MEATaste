<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MeaTaste"
        xmlns:Models="clr-namespace:MeaTaste.DataMEA.Models" x:Class="MeaTaste.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="OpenDialogButton" Content="Load H5 file..." HorizontalAlignment="Left" Height="37" Margin="32,38,0,0" VerticalAlignment="Top" Width="74" Click="OpenDialogButton_Click" RenderTransformOrigin="0.508,-0.402"/>
        <Label x:Name="OpenedFileLabel"   HorizontalAlignment="Left" Margin="49,91,0,0" VerticalAlignment="Top" Content="filename" Visibility="Hidden" IsVisibleChanged="OpenedFileLabel_IsVisibleChanged"/>
        <Label x:Name="OpenedFileVersion" HorizontalAlignment="Left" Margin="49,122,0,0" VerticalAlignment="Top" Content="fileversion" Visibility="Hidden" IsVisibleChanged="OpenedFileVersion_IsVisibleChanged"/>
        <ListBox x:Name="ListOfElectrodes" Margin="32,208,519,31" IsVisibleChanged="ListOfElectrodes_IsVisibleChanged" Visibility="Hidden" SelectionChanged="ListOfElectrodes_SelectionChanged"/>
    </Grid>
</Window>
