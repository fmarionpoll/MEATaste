<Window x:Class="MeaTaste.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MeaTaste"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="OpenDialogButton" Content="Load H5 file..." HorizontalAlignment="Left" Height="37" Margin="32,38,0,0" VerticalAlignment="Top" Width="74" Click="OpenDialogButton_Click" RenderTransformOrigin="0.508,-0.402"/>
        <Label x:Name="OpenedFileLabel" HorizontalAlignment="Left" Margin="49,91,0,0" VerticalAlignment="Top"/>
        <Label x:Name="OpenedFileVersion" Content="Label" HorizontalAlignment="Left" Margin="54,106,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
