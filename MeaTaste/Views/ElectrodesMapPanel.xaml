        <WpfPlot x:Name="ElectrodesMap" HorizontalAlignment="Center" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" Width="250" Height="228" MouseMove="ElectrodesMap_MouseMove" MouseDoubleClick="ElectrodesMap_MouseDoubleClick"/>
