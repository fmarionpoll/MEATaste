<Application x:Class="MEATaste.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:MEATaste.Views"
             xmlns:viewModels="clr-namespace:MEATaste.ViewModels"
             xmlns:fileOpen="clr-namespace:MEATaste.Views.FileOpen"
             Startup="OnStartup">
               
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:ElectrodesMapPanelModel}">
            <views:ElectrodesMapPanel />
        </DataTemplate>
        <DataTemplate DataType="{x:Type fileOpen:FileOpenPanelController}">
            <fileOpen:FileOpenPanelView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:OneElectrodePanelModel}">
            <views:OneElectrodePanel />
        </DataTemplate>
    </Application.Resources>
</Application>
